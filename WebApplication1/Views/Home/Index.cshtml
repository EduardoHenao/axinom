@{
    ViewData["Title"] = "Axinom Test";
}

<form method="post" enctype="multipart/form-data" asp-controller="Upload" asp-action="UploadZip">
    <div class="form-group">
        <div class="col-md-3">
            <p>Upload file:</p>
            <input type="file" name="files" multiple />
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-3">
            <input type="submit" value="Upload" />
        </div>
    </div>
</form>
