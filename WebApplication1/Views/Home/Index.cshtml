@{
    var title = "Axinom test";
    ViewData["Title"] = title;
}

<form enctype="multipart/form-data" asp-controller="Upload" asp-action="UploadFile" method="post">
    <p>@title</p>
    <div class="form-group">
        <div class="col-md-3">
            <p>Upload one or more files using this form:</p>
            <input type="file" name="file"/>
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-3">
            <input type="submit" value="Upload" />
        </div>
    </div>
</form>
